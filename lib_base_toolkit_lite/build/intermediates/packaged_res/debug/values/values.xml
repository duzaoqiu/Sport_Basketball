    <style name="CustomViewInflaterTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="viewInflaterClass">com.bench.android.core.util.CustomAppViewInflater</item>
    </style>
    <style name="base_launch_theme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="bg_style_normal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/padding_10dp</item>
    </style>
    <style name="cut_line_horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/line_color</item>
    </style>
    <style name="cut_line_horizontal_10">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:background">@color/line_color</item>
    </style>
    <style name="cut_line_vertical">
        <item name="android:layout_width">0.5dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/line_color</item>
    </style>
    <style mce_bogus="1" name="dialog_anim_bottom" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/slide_in_bottom</item>
        <item name="android:windowExitAnimation">@anim/slide_out_bottom</item>
    </style>
    <style mce_bogus="1" name="dialog_anim_side" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/slide_in_right_side</item>
        <item name="android:windowExitAnimation">@anim/slide_out_right_side</item>
    </style>
    <style name="dialog_base" parent="Theme.AppCompat.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <!--<item name="android:backgroundDimEnabled">false</item>  &lt;!&ndash;消除背景阴影&ndash;&gt;-->
        <item name="android:backgroundDimAmount">0.2</item><!--阴影度-->
        <!--<item name="android:windowAnimationStyle">@style/dialog_anim_side</item>-->
    </style>
    <style name="okBtn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_ok_btn_red_gray</item>
    </style>
    <style name="text_style_extra_large">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_extra_large</item>
        <item name="android:textColor">@color/text_color_normal</item>
    </style>
    <style name="text_style_large">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_large</item>
        <item name="android:textColor">@color/text_color_normal</item>
    </style>
    <style name="text_style_normal">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_normal</item>
        <item name="android:textColor">@color/text_color_normal</item>
    </style>
    <style name="text_style_small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:textColor">@color/text_color_normal</item>
    </style>
    <declare-styleable name="BannerView">
        <attr format="string" name="ratio"/>
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <attr format="dimension" name="horizontalSpacing"/>
        <attr format="dimension" name="verticalSpacing"/>
    </declare-styleable>
    <declare-styleable name="LoadFrameLayout">
        <attr format="reference" name="emptyView"/>
        <attr format="reference" name="errorView"/>
        <attr format="reference" name="loadingView"/>
    </declare-styleable>
    <declare-styleable name="MenuLayout">
        <attr format="boolean" name="with_gif_view"/>
    </declare-styleable>
    <declare-styleable name="MsgView">
        <!-- 圆角矩形背景色 -->
        <attr format="color" name="mv_backgroundColor"/>
        <!-- 圆角弧度,单位dp-->
        <attr format="dimension" name="mv_cornerRadius"/>
        <!-- 圆角弧度,单位dp-->
        <attr format="dimension" name="mv_strokeWidth"/>
        <!-- 圆角边框颜色-->
        <attr format="color" name="mv_strokeColor"/>
        <!-- 圆角弧度是高度一半-->
        <attr format="boolean" name="mv_isRadiusHalfHeight"/>
        <!-- 圆角矩形宽高相等,取较宽高中大值-->
        <attr format="boolean" name="mv_isWidthHeightEqual"/>
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr format="dimension" name="riv_corner_radius"/>
    </declare-styleable>
    <declare-styleable name="ShowOrHidePwdView">
        <attr format="string" name="text_hint"/>
    </declare-styleable>
    <declare-styleable name="SimpleImageView">
        <attr format="boolean" name="isLong"/>
        <attr format="boolean" name="isRound"/>
        <attr format="dimension" name="cornerRadius"/>
    </declare-styleable>
    <declare-styleable name="TagFlowLayout">
        <attr format="integer" name="max_select"/>
        <attr format="integer" name="span_count"/>
        <attr format="boolean" name="single_line"/>
        <attr format="reference" name="tag_flow_view"/>
        <attr format="reference" name="tag_flow_text_view_id"/>
        <attr format="boolean" name="tag_is_clickable"/>
        <attr format="integer" name="max_line"/>
        <attr format="boolean" name="add_lastOne"/>
        <attr format="boolean" name="tag_is_changeable"/>
        <attr name="tag_gravity">
            <enum name="left" value="-1"/>
            <enum name="center" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="WrapXXXView">
        <attr format="boolean" name="anti_repeat"/>
        <attr format="integer" name="delay_time"/>
    </declare-styleable>
</resources>